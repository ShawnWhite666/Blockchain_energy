<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blockchainenergy.security.notice.dao.NoticeDao">

    <select id="findNoticeListByCondition" resultType="com.blockchainenergy.security.notice.Notice">
        SELECT
        title,
        content,
        created_at
        FROM
        notices
    </select>
</mapper>